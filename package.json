{
  "name": "vue3-ts-tailwind-template",
  "version": "1.5.0",
  "description": "Vue3 + TS + Tailwind 模板",
  "private": true,
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git@github.com:yuanlee301415/Vue3-TS-Tailwind-Template.git"
  },
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --fix",
    "format": "prettier --write src/",
    "gen": "node ./scripts/gen.js",
    "server": "json-server server/db.json5 --port 3001"
  },
  "dependencies": {
    "axios": "^1.7.7",
    "pinia": "^2.2.4",
    "vue": "^3.5.12",
    "vue-router": "^4.4.5"
  },
  "devDependencies": {
    "@tsconfig/node20": "^20.1.4",
    "@types/node": "^20.16.11",
    "@vitejs/plugin-vue": "^5.1.4",
    "@vitejs/plugin-vue-jsx": "^4.0.1",
    "@vue/eslint-config-prettier": "^10.0.0",
    "@vue/eslint-config-typescript": "^14.0.1",
    "@vue/tsconfig": "^0.5.1",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.12.0",
    "eslint-plugin-vue": "^9.29.0",
    "json-server": "1.0.0-beta.3",
    "less": "^4.2.0",
    "npm-run-all2": "^6.2.3",
    "prettier": "^3.3.3",
    "tailwindcss": "^3.4.14",
    "typescript": "~5.5.4",
    "vite": "^5.4.8",
    "vite-plugin-html": "^3.2.2",
    "vue-tsc": "^2.1.6"
  },
  "packageManager": "pnpm@8.1.0",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.1.0"
  }
}
